  legalname         String?   @db.VarChar(512)
  companycode       String?   @db.VarChar(512)
  companycodename   String?   @db.VarChar(512)
  gstin             String?   @db.VarChar(512)
  cin               String?   @db.VarChar(512)
  registeredaddress Json?     @db.Json
  createdat         DateTime? @db.Timestamp(6)
  updatedat         DateTime? @db.Timestamp(6)
}

model dc_invoicepdfhtmls {
  id             Int       @id
  invoicenumber  String?   @db.VarChar(512)
  createdat      DateTime? @db.Timestamp(6)
  invoicepdfhtml String?   @db.VarChar(8192)
  updatedat      DateTime? @db.Timestamp(6)
}

model dc_roundrobin {
  roundrobinid       Int @id @default(autoincrement())
  teamid             Int @db.Integer
  experiencecenterid Int @db.Integer
}

model dc_surveyroundrobin {
  roundrobinid       Int @id @default(autoincrement())
  surveyexecutiveid  Int
  experiencecenterid Int
}
